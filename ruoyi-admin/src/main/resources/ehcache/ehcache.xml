<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd"
	updateCheck="false">
	
	<defaultCache eternal="false" maxElementsInMemory="1000"
		overflowToDisk="false" diskPersistent="false" timeToIdleSeconds="0"
		timeToLiveSeconds="600" memoryStoreEvictionPolicy="LRU" />

	<cache name="account" eternal="false" maxElementsInMemory="100"
		overflowToDisk="false" diskPersistent="false" timeToIdleSeconds="0"
		timeToLiveSeconds="600" memoryStoreEvictionPolicy="LRU" />

	<cache name="blog" eternal="false" maxElementsInMemory="100"
		overflowToDisk="false" diskPersistent="false" timeToIdleSeconds="0"
		timeToLiveSeconds="600" memoryStoreEvictionPolicy="LRU" />

	<cache name="blogType" eternal="false" maxElementsInMemory="100"
		overflowToDisk="false" diskPersistent="false" timeToIdleSeconds="0"
		timeToLiveSeconds="1000" memoryStoreEvictionPolicy="LRU" />

	<cache name="daily" eternal="false" maxElementsInMemory="100"
		overflowToDisk="false" diskPersistent="false" timeToIdleSeconds="0"
		timeToLiveSeconds="1000" memoryStoreEvictionPolicy="LRU" />
		
	<cache name="email" eternal="false" maxElementsInMemory="100"
		overflowToDisk="false" diskPersistent="false" timeToIdleSeconds="0"
		timeToLiveSeconds="1000" memoryStoreEvictionPolicy="LRU" />
		
	<cache name="fileInfo" eternal="false" maxElementsInMemory="100"
		overflowToDisk="false" diskPersistent="false" timeToIdleSeconds="0"
		timeToLiveSeconds="300" memoryStoreEvictionPolicy="LRU" />
	
	<cache name="fileType" eternal="false" maxElementsInMemory="100"
		overflowToDisk="false" diskPersistent="false" timeToIdleSeconds="0"
		timeToLiveSeconds="300" memoryStoreEvictionPolicy="LRU" />

	<cache name="logininMongo" eternal="false" maxElementsInMemory="100"
		   overflowToDisk="false" diskPersistent="false" timeToIdleSeconds="0"
		   timeToLiveSeconds="300" memoryStoreEvictionPolicy="LRU" />

	<cache name="operLogMongo" eternal="false" maxElementsInMemory="100"
		   overflowToDisk="false" diskPersistent="false" timeToIdleSeconds="0"
		   timeToLiveSeconds="300" memoryStoreEvictionPolicy="LRU" />

	<cache name="logininElastic" eternal="false" maxElementsInMemory="100"
		   overflowToDisk="false" diskPersistent="false" timeToIdleSeconds="0"
		   timeToLiveSeconds="300" memoryStoreEvictionPolicy="LRU" />

	<cache name="operLogElastic" eternal="false" maxElementsInMemory="100"
		   overflowToDisk="false" diskPersistent="false" timeToIdleSeconds="0"
		   timeToLiveSeconds="300" memoryStoreEvictionPolicy="LRU" />

	<cache name="schedule" eternal="false" maxElementsInMemory="100"
		   overflowToDisk="false" diskPersistent="false" timeToIdleSeconds="0"
		   timeToLiveSeconds="60" memoryStoreEvictionPolicy="LRU" />

	<cache name="chatCode" eternal="false" maxElementsInMemory="100"
		   overflowToDisk="false" diskPersistent="false" timeToIdleSeconds="0"
		   timeToLiveSeconds="60" memoryStoreEvictionPolicy="LRU" />

</ehcache>